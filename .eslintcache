    snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"videos\").onSnapshot((snapshot) =>\n      setVideos(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"categories\").onSnapshot((snapshot) =>\n      setCategories(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"readers\").onSnapshot((snapshot) =>\n      setReaders(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"homeimgs\").onSnapshot((snapshot) =>\n      setHomeimgs(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  const handleChange = (e) => {\n    let input = e.target.value;\n    setSearch(input);\n    const results = posts.filter(\n      (post) => post.data.title.toLowerCase().indexOf(input.toLowerCase()) > -1\n    );\n    setSearchposts(results);\n  };\n\n  const SrchResult = ({ post }) => (\n    <div className=\"srch-res\">\n      <div className=\"srch-title-cont\">\n        <div className=\"srch-title\">{post.data.title}</div>\n        <div>Published on : {post.data.date}</div>\n      </div>\n      <div className=\"srch-cat-cont\">\n        Categories :\n        {post?.data?.categories?.map((cat) => (\n          <div className=\"srch-cat\">{cat}</div>\n        ))}\n      </div>\n    </div>\n  );\n\n  return (\n    <div>\n      <Sidenav />\n\n      <div\n        id=\"myOverlay\"\n        className=\"overlay\"\n        style={{ opacity: !showsrch && \"0\", visibility: !showsrch && \"hidden\" }}\n      >\n        <div className=\"closebtn\" onClick={closeSearch} title=\"Close Overlay\">\n          <CancelRoundedIcon className=\"cancel-btn\" />\n        </div>\n        <div className=\"overlay-content\">\n          <input\n            type=\"text\"\n            placeholder=\"Search by Title...\"\n            name=\"search\"\n            value={search}\n            onChange={handleChange}\n          />\n          <div className=\"searchcont\">\n            <SearchRoundedIcon className=\"searchbtn\" />\n          </div>\n          <div className=\"sugs\">\n            <div className=\"sugs-title\">\n              {search.length > 0 ? \"Suggestions\" : \"Popular Posts\"}\n            </div>\n\n            {search.length > 0 ? (\n              <div>\n                {searchposts.map((post) => (\n                  <a href={`/blogs/${post.id}`}>\n                    <SrchResult post={post} />\n                  </a>\n                ))}\n              </div>\n            ) : (\n              <div>\n                {posts.map((post) => (\n                  <Suggestions post={post} key={post.id} />\n                ))}\n              </div>\n            )}\n          </div>\n        </div>\n      </div>\n\n      <div className=\"homePage\" style={{ marginLeft: \"16%\" }}>\n        <div className=\"search-cont\" onClick={openSearch}>\n          <SearchRoundedIcon className=\"search-btn\" />\n        </div>\n\n        <Home id=\"home\" homeimgs={homeimgs} />\n        <About id=\"about\" />\n        <Newsletter\n          id=\"newsletter\"\n          style={{ borderBottom: \"3px solid #ffa351\" }}\n        />\n        <Blog\n          id=\"blog\"\n          posts={posts}\n          categories={categories[0]?.data?.categories}\n          readers={readers}\n        />\n        <Insta id=\"insta\" />\n        <Audio id=\"Audio\" videos={videos} />\n        {/* <Readers id=\"readers\" /> */}\n        {/* <Contact id=\"contact\" /> */}\n        <Footer id=\"footer\" />\n      </div>\n    </div>\n  );\n}\n\nexport default HomePage;\n","C:\\Users\\tejas\\OneDrive\\Desktop\\projects\\beneathTheSunRays\\src\\Components\\Loading.js",[],"C:\\Users\\tejas\\OneDrive\\Desktop\\projects\\beneathTheSunRays\\src\\Components\\Home.js",["123"],"import React from \"react\";\r\n// import { Carousel } from \"react-responsive-carousel\";\r\nimport title from \"../img/title.png\";\r\nimport Fade from \"react-reveal/Fade\";\r\n\r\nimport Carousel from \"react-multi-carousel\";\r\nimport \"react-multi-carousel/lib/styles.css\";\r\n\r\nconst responsive = {\r\n  superLargeDesktop: {\r\n    // the naming can be any, depends on you.\r\n    breakpoint: { max: 4000, min: 3000 },\r\n    items: 1,\r\n  },\r\n  desktop: {\r\n    breakpoint: { max: 3000, min: 1024 },\r\n    items: 1,\r\n  },\r\n  tablet: {\r\n    breakpoint: { max: 1024, min: 464 },\r\n    items: 1,\r\n  },\r\n  mobile: {\r\n    breakpoint: { max: 464, min: 0 },\r\n    items: 1,\r\n  },\r\n};\r\n\r\nexport default function Home({ homeimgs }) {\r\n  console.log(homeimgs);\r\n  return (\r\n    <div className=\"home\">\r\n      <div className=\"homeleft\">\r\n        <Fade bottom cascade>\r\n          <div\r\n            style={{ marginTop: \"-6em\", textAlign: \"center\" }}\r\n            className=\"insta-tag\"\r\n          >\r\n            <div>Come, I will take you to the world of positive vibes</div>\r\n            <div style={{ marginTop: \"0em\" }} className=\"insta\">\r\n              <a\r\n                href=\"https://www.instagram.com/beneaththesunrays/?hl=en\"\r\n                target=\"_blank\"\r\n              >\r\n                <img\r\n                  className=\"in\"\r\n              