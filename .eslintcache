s/AboutPage\";\nimport BlogPage from \"./Pages/Blogpage\";\nimport Adminpage from \"./Pages/Adminpage\";\nimport CategoryPage from \"./Pages/categoryPage\";\nimport BlogSection from \"./Pages/BlogSection\";\n\nimport { useStateValue } from \"./StateProvider\";\n\nfunction App() {\n  const [loading, setLoading] = useState(true);\n  const [{ user }, dispatch] = useStateValue();\n\n  useEffect(() => {\n    setInterval(() => {\n      setLoading(false);\n    }, 0);\n  }, []);\n\n  return (\n    <div className=\"App\">\n      {loading ? (\n        <Loading />\n      ) : (\n        <div className=\"main-content\">\n          <Switch>\n            <Route path=\"/\" exact>\n              <HomePage />\n            </Route>\n\n            <Route path=\"/about\" exact>\n              <AboutPage />\n            </Route>\n\n            <Route path=\"/blogs/:id\" exact>\n              <BlogPage />\n            </Route>\n\n            <Route path=\"/categories/:id\" exact>\n              <CategoryPage />\n            </Route>\n\n            <Route path=\"/admin\" exact>\n             {!user ? <Login /> : <Adminpage />} \n            {/* <Adminpage /> */}\n\n\n            </Route>\n\n            <Route path=\"/blogsection\" exact>\n              <BlogSection />\n            </Route>\n          </Switch>\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport default App;\n\n{\n  /* {!user ? <Login /> : <Adminpage />} */\n  // <Adminpage />\n\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/reducer.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/AboutPage.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Login.js",["108"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Loading.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/Adminpage.js",["109","110","111","112"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/BlogSection.js",["113","114","115"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/HomePage.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/categoryPage.js",["116","117"],"import React, { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport ArrowBackIcon from \"@material-ui/icons/ArrowBack\";\nimport { db } from \"../firebase\";\nimport Footer from \"../Components/Footer\";\n\n\nexport default function CategoryPage() {\n  let { id } = useParams();\n  const [posts, setPosts] = useState([]);\n  const [catposts, setCatPosts] = useState([]);\n  const [popularPosts, setPopular_posts] = useState([]);\n  const [pposts, setPposts] = useState([]);\n\n  const setcategoriesfunc = (posts) => {\n    let newposts = [];\n    for (let i in posts) {\n      for (var post in posts[i].data.categories)\n        if (posts[i].data.categories[post].name === id) {\n          newposts.push(posts[i]);\n        }\n      // console.log(posts[i]);\n    }\n    setCatPosts(newposts);\n    // console.log(catposts);\n  };\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"posts\").onSnapshot((snapshot) =>\n      setPosts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, [posts]);\n\n  useEffect(() => {\n    if (posts) {\n      setcategoriesfunc(posts);\n    }\n  }, [posts, setcategoriesfunc]);\n\n  \n  useEffect(() => {\n    const unsubscribe = db.collection(\"popular_posts\").onSnapshot((snapshot) =>\n      setPopular_posts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, [popularPosts]);\n\n  useEffect(() => {\n    if (posts && popularPosts) {\n      let tmp = [];\n      for (let tmp_id in popularPosts[0]?.data?.popular) {\n        for (let post in posts) {\n          if (posts[post].id === popularPosts[0]?.data?.popular[tmp_id]) {\n            tmp.push(posts[post]);\n          }\n        }\n      }\n      setPposts(tmp);\n    }\n  }, [posts, popularPosts]);\n\n\n  return (\n    <div className=\"cat-page\">\n      <div className=\"abt-left\">\n        <a href=\"/\">\n          <ArrowBackIcon className=\"back\" />\n          Back\n        </a>\n      </div>\n\n      <div className=\"cat-right\">\n        {catposts.length !== 0 ? (\n          <div>\n            <div className=\"title-cont\">\n              <div className=\"blog-title\">{id}</div>\n            </div>\n\n            {catposts?.map((post) => (\n              <div className=\"cat-post-cont\">\n                <a href={`/blogs/`} href={`/blogs/${post.id}`}>\n                  <div className=\"sug-cont\">\n                    <div className=\"sug-left\">\n                      <div\n                        className=\"img\"\n                        style={{ backgroundImage: `url(${post.data.imgurl})` }}\n                      ></div>\n                    </div>\n                    <div className=\"sug-right\">\n                      <div className=\"sug-title\">{post.data.title}</div>\n                      <div className=\"sug-text\">\n                        <div className=\"sug-text-cont\">\n                          <span>{post.data.desc}</span>\n                        </div>\n                        <div>... Read more</div>\n                      </div>\n                    </div>\n                  </div>\n                </a>\n              </div>\n            ))}\n\n            { <Footer id=\"footer\" pposts={pposts} />}\n          </div>\n        ) : (\n          <div className=\"blog-loading\">Loading...</div>\n        )}\n      </div>\n    </div>\n  );\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/Blogpage.js",["118","119"],"import React, { useEffect, useState } from \"react\";\nimport ArrowBackIcon from \"@material-ui/icons/ArrowBack\";\nimport { useParams } from \"react-router-dom\";\nimport Fade from \"react-reveal/Fade\";\nimport Footer from \"../Components/Footer\";\nimport Comment from \"../Components/Comment\";\n\nimport { db } from \"../firebase\";\n\nexport default function BlogPage() {\n  let { id } = useParams();\n  const [post, setPost] = useState(null);\n\n  const [posts, setPosts] = useState([]);\n  const [popularPosts, setPopular_posts] = useState([]);\n  const [pposts, setPposts] = useState([]);\n  const [ispoem, setIspoem] = useState(null);\n\n\n  const [scroll, setScroll] = useState(0);\n\n  const onScroll = () => {\n    const Scrolled = document.documentElement.scrollTop;\n    const MaxHeight =\n      document.documentElement.scrollHeight -\n      document.documentElement.clientHeight;\n    const ScrollPercent = (Scrolled / MaxHeight) * 100;\n    setScroll(ScrollPercent);\n  };\n\n  window.addEventListener(\"scroll\", onScroll);\n\n  // console.log(post)\n\n  useEffect(() => {\n    if (id) {\n      db.collection(\"posts\")\n        .doc(id)\n        .onSnapshot((snapshot) => {\n          setPost(snapshot.data());\n          console.log(snapshot.data());\n          if(snapshot.data().categories.find(x => x.name == \"Poems\")){\n            setIspoem(true)\n            console.log(ispoem)\n          }\n        });\n    }\n    window.scrollTo(0, 0);\n  }, [id]);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"posts\").onSnapshot((snapshot) =>\n      setPosts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"popular_posts\").onSnapshot((snapshot) =>\n      setPopular_posts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    if (posts && popularPosts) {\n      let tmp = [];\n      for (let tmp_id in popularPosts[0]?.data?.popular) {\n        for (let post in posts) {\n          if (posts[post].id ===  popularPosts[0]?.data?.popular[tmp_id]) {\n            tmp.push(posts[post]);\n          }\n        }\n      }\n      setPposts(tmp);\n    }\n  }, [posts, popularPosts]);\n\n  return (\n    <div className=\"blog-cont\">\n      <div className=\"abt-left\">\n        <a href=\"/\">\n          <ArrowBackIcon className=\"back\" />\n          Back\n        </a>\n      </div>\n      <Fade>\n        <div className=\"blog-right\">\n          <div className=\"scroll-main\">\n            <div className=\"scroll-in\" style={{width:`${scroll}%`}}></div>\n          </div>\n          {post !== null ? (\n            <div  className=\"blog-right-child\">\n              <Fade>\n                <div className=\"title-cont\">\n                  <div className=\"blog-title\">{post.title}</div>\n                  <div className=\"sub-cont\">\n                    <div>\n                      <div className=\"date\">Published on : {post.date}</div>\n                    </div>\n                    <div className=\"cats\">\n                      {post.categories &&\n                        post.categories.map((cat,i) => {\n                          return <div key={i} className=\"cat\">{cat[\"name\"]}</div>;\n                        })}\n                    </div>\n                  </div>\n                </div>\n              </Fade>\n              <div className=\"blog-img\">\n                <img src={post.imgurl} alt=\"\" />\n              </div>\n              <div className=\"blog-cont1\">\n                {ispoem === true ? (\n\n                  <pre>\n                    <span>{post.desc}</span>\n                  </pre>\n                    ): (\n                    <span>{post.desc}</span>\n\n                    ) \n                }\n            \n                <Comment post={post} />\n              </div>\n\n              <Footer id=\"footer\" pposts={pposts}/>\n            </div>\n          ) : (\n            <div className=\"blog-loading\">Loading...</div>\n          )}\n        </div>\n      </Fade>\n    </div>\n  );\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Footer.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Home.js",["120","121"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Sidenav.js",["122"],"import React from \"react\";\nimport HomeIcon from \"@material-ui/icons/Home\";\nimport FaceRoundedIcon from \"@material-ui/icons/FaceRounded\";\nimport CreateRoundedIcon from \"@material-ui/icons/CreateRounded\";\nimport DraftsIcon from \"@material-ui/icons/Drafts\";\nimport HeadsetMicIcon from \"@material-ui/icons/HeadsetMic\";\n\nimport GirlOutlinedIcon from '@mui/icons-material/GirlOutlined';\n\nimport { Link } from \"react-scroll\";\nimport Fade from \"react-reveal/Fade\";\n\nexport default function Header() {\n  return (\n    <header className=\"sidenav\">\n      <Fade top cascade>\n        <div className=\"brand\"></div>\n\n        <div className=\"navs\">\n          <Link activeClass=\"active\" to=\"home\" spy={true} smooth={true}>\n            <div className=\"navlink\">\n              <HomeIcon className=\"icon\" />\n              Home\n            </div>\n          </Link>\n\n          <Link activeClass=\"active\" to=\"about\" spy={true} smooth={true}>\n            <div className=\"navlink\">\n              <FaceRoundedIcon className=\"icon\" />\n              About\n            </div>\n          </Link>\n\n          <Link activeClass=\"active\" to=\"blog\" spy={true} smooth={true}>\n            <div className=\"navlink\">\n              <CreateRoundedIcon className=\"icon\" />\n              Blog\n            </div>\n          </Link>\n\n          <Link activeClass=\"active\" to=\"podcasts\" spy={true} smooth={true}>\n            <div className=\"navlink\">\n              <HeadsetMicIcon className=\"icon\" />\n              Podcasts\n            </div>\n          </Link>\n\n          <Link activeClass=\"active\" to=\"poetry\" spy={true} smooth={true}>\n            <div className=\"navlink\">\n              <GirlOutlinedIcon className=\"icon\" />\n              Spoken Poetry\n            </div>\n          </Link>\n     \n        </div>\n\n        <div className=\"socials\">\n          {/* <div className=\"icon-holder\">\n            <img src=\"https://img.icons8.com/color/48/fa314a/facebook-new.png\" />\n          </div>\n\n          <div className=\"icon-holder\">\n            <img src=\"https://img.icons8.com/fluent/48/fa314a/instagram-new.png\" />\n          </div> */}\n        </div>\n        \n      </Fade>\n    </header>\n  );\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/firebase.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/About.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Blog.js",["123","124","125"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Podcasts.js",["126","127","128"],"import React, { useState, useEffect, useRef } from \"react\";\nimport Carousel from \"react-multi-carousel\";\nimport \"react-multi-carousel/lib/styles.css\";\nimport PlayCircleOutlineRoundedIcon from \"@material-ui/icons/PlayCircleOutlineRounded\";\nimport Fade from \"react-reveal/Fade\";\n\nconst responsive = {\n  superLargeDesktop: {\n    // the naming can be any, depends on you.\n    breakpoint: { max: 4000, min: 3000 },\n    items: 5,\n  },\n  desktop: {\n    breakpoint: { max: 3000, min: 1024 },\n    items: 2,\n  },\n  tablet: {\n    breakpoint: { max: 1024, min: 650 },\n    items: 2,\n  },\n  mobile: {\n    breakpoint: { max: 650, min: 0 },\n    items: 1,\n  },\n};\n\nexport default function Podcasts({ videos }) {\n  const [currvideo, setCurrvideo] = useState(-1);\n  const [vidsrc, setVidsrc] = useState(\"\");\n\n  const [playing, setPlaying] = u