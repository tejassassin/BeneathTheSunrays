ts/teja/projects/beneaththesunrays/src/Pages/HomePage.js",["109","110"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/categoryPage.js",["111"],"import React, { useEffect, useState } from \"react\";\nimport { useParams, useHistory } from \"react-router-dom\";\nimport ArrowBackIcon from \"@material-ui/icons/ArrowBack\";\nimport { db } from \"../firebase\";\nimport Fade from \"react-reveal/Fade\";\n\nimport Footer from \"../Components/Footer\";\n\nexport default function CategoryPage() {\n  let { id } = useParams();\n  const [posts, setPosts] = useState([]);\n  const [catposts, setCatPosts] = useState(null);\n  const [popularPosts, setPopular_posts] = useState([]);\n  const [pposts, setPposts] = useState([]);\n  const [loading, setLoading] = useState(true);\n  \n  const history = useHistory();\n\n  const setcategoriesfunc = (posts) => {\n    let newposts = [];\n    for (let i in posts) {\n      for (var post in posts[i].data.categories)\n        if (posts[i].data.categories[post].name === id) {\n          newposts.push(posts[i]);\n        }\n      // console.log(posts[i]);\n    }\n    setCatPosts(newposts);\n    // console.log(catposts);\n  };\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"posts\").onSnapshot((snapshot) =>\n      setPosts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n\n    console.log(16);\n\n    return () => {\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    if (posts) {\n      setcategoriesfunc(posts);\n      console.log(18)\n    }\n  }, [posts]);\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"popular_posts\").onSnapshot((snapshot) =>\n      setPopular_posts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n\n    console.log(17);\n    return () => {\n\n      unsubscribe();\n    };\n  }, []);\n\n  useEffect(() => {\n    if (posts && popularPosts) {\n      let tmp = [];\n      for (let tmp_id in popularPosts[0]?.data?.popular) {\n        for (let post in posts) {\n          if (posts[post].id === popularPosts[0]?.data?.popular[tmp_id]) {\n            tmp.push(posts[post]);\n          }\n        }\n      }\n      setPposts(tmp);\n      console.log(20)\n\n    }\n  }, [posts, popularPosts]);\n\n  useEffect(() => {\n    setInterval(() => {\n      setLoading(false);\n    }, 2000);\n  }, []);\n\n  return (\n    <div className=\"cat-page\">\n      <div className=\"abt-left\">\n        <div onClick={() => history.goBack()} className=\"backcont\">\n          <ArrowBackIcon className=\"back\" />\n          Back\n        </div>\n      </div>\n\n      {!loading ? (\n        <div className=\"cat-right\">\n            <div className=\"title-cont\">\n              <Fade bottom cascade>\n                <div className=\"blog-title\">{id}</div>\n              </Fade>\n            </div>\n\n            <div>\n              {catposts?.map((post, idx) => (\n                <div key={idx} className=\"cat-post-cont\">\n                  <Fade>\n\n                    <a href={`/blogs/${post.id}`}>\n                      <div className=\"sug-cont\">\n                        <div className=\"sug-left\">\n                          <div\n                            className=\"img\"\n                            style={{\n                              backgroundImage: `url(${post.data.imgurl})`,\n                            }}\n                            ></div>\n                        </div>\n                        <div className=\"sug-right\">\n                          <div className=\"sug-title\">{post.data.title}</div>\n                          <div className=\"sug-text\">\n                            <div className=\"sug-text-cont\">\n                              <span>{post.data.desc}</span>\n                            </div>\n\n                            {window.innerWidth < 530 ? (\n                              <div className=\"readmore\">... Read </div>\n                              ) : (\n                                <div className=\"readmore\">...Read more</div>\n                                )}\n                          </div>\n                        </div>\n                      </div>\n                    </a>\n                                </Fade>\n                </div>\n              ))}\n            </div>\n\n          {!loading && <Footer id=\"footer\" pposts={pposts} />}\n\n        </div>\n\n      ) : (\n        <div className=\"blog-loading\"> Loading...</div>\n      )}\n    </div>\n  );\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/Blogpage.js",["112"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Footer.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Home.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Sidenav.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/firebase.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/About.js",["113"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Blog.js",["114","115","116"],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Podcasts.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Suggestions.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Newsletter.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Poetry.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Comment.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/CardDeck.js",[],{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","severity":1,"message":"122","line":3,"column":10,"nodeType":"123","messageId":"124","endLine":3,"endColumn":19},{"ruleId":"121","severity":1,"message":"125","line":5,"col