     for (let post in posts) {\n          if (posts[post].id ===  popularPosts[0]?.data?.popular[tmp_id]) {\n            tmp.push(posts[post]);\n          }\n        }\n      }\n      setPposts(tmp);\n    }\n  }, [posts, popularPosts]);\n\n  return (\n    <div className=\"blog-cont\">\n      <div className=\"abt-left\">\n        <a href=\"/\">\n          <ArrowBackIcon className=\"back\" />\n          Back\n        </a>\n      </div>\n      <Fade>\n        <div className=\"blog-right\">\n          <div className=\"scroll-main\">\n            <div className=\"scroll-in\" style={{width:`${scroll}%`}}></div>\n          </div>\n          {post !== null ? (\n            <div  className=\"blog-right-child\">\n              <Fade>\n                <div className=\"title-cont\">\n                  <div className=\"blog-title\">{post.title}</div>\n                  <div className=\"sub-cont\">\n                    <div>\n                      <div className=\"date\">Published on : {post.date}</div>\n                    </div>\n                    <div className=\"cats\">\n                      {post.categories &&\n                        post.categories.map((cat,i) => {\n                          return <div key={i} className=\"cat\">{cat[\"name\"]}</div>;\n                        })}\n                    </div>\n                  </div>\n                </div>\n              </Fade>\n              <div className=\"blog-img\">\n                <img src={post.imgurl} alt=\"\" />\n              </div>\n              <div className=\"blog-cont1\">\n                {ispoem === true ? (\n\n                  <pre>\n                    <span>{post.desc}</span>\n                  </pre>\n                    ): (\n                    <span>{post.desc}</span>\n\n                    ) \n                }\n            \n                <Comment post={post} />\n              </div>\n\n              <Footer id=\"footer\" pposts={pposts}/>\n            </div>\n          ) : (\n            <div className=\"blog-loading\">Loading...</div>\n          )}\n        </div>\n      </Fade>\n    </div>\n  );\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Footer.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Home.js",["123","124"],"import React from \"react\";\nimport title from \"../img/title.png\";\nimport Fade from \"react-reveal/Fade\";\n\nimport Carousel from \"react-multi-carousel\";\nimport \"react-multi-carousel/lib/styles.css\";\n\nconst responsive = {\n  superLargeDesktop: {\n    // the naming can be any, depends on you.\n    breakpoint: { max: 4000, min: 3000 },\n    items: 1,\n  },\n  desktop: {\n    breakpoint: { max: 3000, min: 1024 },\n    items: 1,\n  },\n  tablet: {\n    breakpoint: { max: 1024, min: 464 },\n    items: 1,\n  },\n  mobile: {\n    breakpoint: { max: 464, min: 0 },\n    items: 1,\n  },\n};\n\nexport default function Home({ homeimgs }) {\n  return (\n    <div className=\"home\">\n      <div className=\"homeleft\">\n        <Fade bottom cascade>\n          <div\n            style={{ marginTop: \"-6em\", textAlign: \"center\" }}\n            className=\"insta-tag\"\n          >\n            <div>Come, I will take you to the world of positive vibes ðŸŒ»</div>\n            <div style={{ marginTop: \"0em\" }} className=\"insta-home\">\n              <a\n                href=\"https://www.instagram.com/beneaththesunrays/?hl=en\"\n                target=\"_blank\"\n              >\n                <img\n                  className=\"in\"\n                  width=\"50px\"\n                  src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/a/a5/Instagram_icon.png/2048px-Instagram_icon.png\"\n                  alt=\"instagram\"\n                />\n              </a>\n            </div>\n          </div>\n          <div className=\"hometitle\">\n          <a\n                href=\"https://www.instagram.com/beneaththesunrays/?hl=en\"\n                target=\"_blank\"\n    