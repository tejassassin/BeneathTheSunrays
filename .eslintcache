.data.categories[post].name === id) {\n          newposts.push(posts[i]);\n        }\n      // console.log(posts[i]);\n    }\n    setCatPosts(newposts);\n    // console.log(catposts);\n  };\n\n  useEffect(() => {\n    const unsubscribe = db.collection(\"posts\").onSnapshot((snapshot) =>\n      setPosts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, [posts]);\n\n  useEffect(() => {\n    if (posts) {\n      setcategoriesfunc(posts);\n    }\n  }, [posts, setcategoriesfunc]);\n\n  \n  useEffect(() => {\n    const unsubscribe = db.collection(\"popular_posts\").onSnapshot((snapshot) =>\n      setPopular_posts(\n        snapshot.docs.map((doc) => ({\n          id: doc.id,\n          data: doc.data(),\n        }))\n      )\n    );\n    return () => {\n      unsubscribe();\n    };\n  }, [popularPosts]);\n\n  useEffect(() => {\n    if (posts && popularPosts) {\n      let tmp = [];\n      for (let tmp_id in popularPosts[0]?.data?.popular) {\n        for (let post in posts) {\n          if (posts[post].id === popularPosts[0]?.data?.popular[tmp_id]) {\n            tmp.push(posts[post]);\n          }\n        }\n      }\n      setPposts(tmp);\n    }\n  }, [posts, popularPosts]);\n\n\n  return (\n    <div className=\"cat-page\">\n      <div className=\"abt-left\">\n        <a href=\"/\">\n          <ArrowBackIcon className=\"back\" />\n          Back\n        </a>\n      </div>\n\n      <div className=\"cat-right\">\n        {catposts.length !== 0 ? (\n          <div>\n            <div className=\"title-cont\">\n              <div className=\"blog-title\">{id}</div>\n            </div>\n\n            {catposts?.map((post) => (\n              <div className=\"cat-post-cont\">\n                <a href={`/blogs/`} href={`/blogs/${post.id}`}>\n                  <div className=\"sug-cont\">\n                    <div className=\"sug-left\">\n                      <div\n                        className=\"img\"\n                        style={{ backgroundImage: `url(${post.data.imgurl})` }}\n                      ></div>\n                    </div>\n                    <div className=\"sug-right\">\n                      <div className=\"sug-title\">{post.data.title}</div>\n                      <div className=\"sug-text\">\n                        <div className=\"sug-text-cont\">\n                          <span>{post.data.desc}</span>\n                        </div>\n                        <div>... Read more</div>\n                      </div>\n                    </div>\n                  </div>\n                </a>\n              </div>\n            ))}\n\n            { <Footer id=\"footer\" pposts={pposts} />}\n          </div>\n        ) : (\n          <div className=\"blog-loading\">Loading...</div>\n        )}\n      </div>\n    </div>\n  );\n}\n","/Users/teja/Documents/teja/projects/beneaththesunrays/src/Pages/Blogpage.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Footer.js",[],"/Users/teja/Documents/teja/projects/beneaththesunrays/src/Components/Home.js",["111","112"],"import React from \"react\";\nimport title from \"../img/title.png\";\nimport Fade from \"react-reveal/Fade\";\n\nimport Carousel from \"react-multi-carousel\";\nimport \"react-multi-carousel/lib/styles.css\";\n\nconst responsive = {\n  superLargeDesktop: {\n    // the naming can be any, depends on you.\n    breakpoint: { max: 4000, min: 3000 },\n    items: 1,\n  },\n  desktop: {\n    breakpoint: { max: 3000, min: 1024 },\n    items: 1,\n  },\n  tablet: {\n    breakpoint: { max: 1024, min: 464 },\n    items: 1,\n  },\n  mobile: {\n    breakpoint: { max: 464, min: 0 },\n    items: 1,\n  },\n};\n\nexport default function Home({ homeimgs }) {\n  return (\n    <div className=\"home\">\n      <div className=\"homeleft\">\n        <Fade bottom cascade>\n          <div\n            style={{ marginTop: \"-6em\", textAli