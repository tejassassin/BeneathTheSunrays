eos]);\n\n  useEffect(() => {\n    if (vidref.current) {\n      vidref.current.play();\n      setPlaying(true);\n    }\n  }, [vidsrc]);\n\n  const handleClick = (e) => {\n    if (playing) {\n      setPlaying(false);\n    } else {\n      setPlaying(true);\n    }\n  };\n\n  const handlecarClick = (id) => {\n    setCurrvideo(id);\n    if (vidref.current) {\n      titleRef.current.scrollIntoView({ behavior: \"smooth\" });\n    }\n  };\n\n  return (\n    <div>\n      <Fade bottom cascade>\n        <div className=\"podcasts\">\n          <div className=\"section-title\" ref={titleRef}>\n            Podcasts\n          </div>\n          <div className=\"section-subtitle\">Mujhe sunna pasand karoge ?</div>\n          {currvideo === -1 ? (\n            \"\"\n          ) : (\n            <div className=\"podcasts-cont\">\n              <div className=\"podcasts-player\">\n                <div className=\"btn-cont\"></div>\n                <video\n                  ref={vidref}\n                  className=\"video\"\n                  loop\n                  preload=\"none\"\n                  onClick={handleClick}\n                  src={vidsrc}\n                  controls\n                  type=\"video/mp4\"\n                ></video>\n              </div>\n            </div>\n          )}\n          <Carousel\n            className=\"car-aud\"\n            emulateTouch\n            infiniteLoop\n            thumbWidth={200}\n            responsive={responsive}\n          >\n            {videos.map((vid, index) => {\n              return (\n                <Fade right cascade key={index}>\n                  <div className=\"slide-3\" key={vid.id}>\n                    <div className=\"carvid-cont\" key={vid.id}>\n                      <div className=\"vid-title\">{vid.data.vidname}</div>\n                      <div\n                        name={vid.id}\n                        className=\"car-video\"\n                        onClick={() => handlecarClick(vid.id)}\n        